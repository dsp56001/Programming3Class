<Window x:Class="WpfAppDogDepend.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDogDepend"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel >
            <Label>Select a dog window to test</Label>
            <Button Content="Dog" Click="Dog_Button_Click"/>
            <Button Content="Dogs" Click="Dogs_Button_Click" />
        </StackPanel>
    </Grid>
</Window>
